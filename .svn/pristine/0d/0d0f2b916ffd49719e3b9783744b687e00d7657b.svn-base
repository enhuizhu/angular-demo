import { combineReducers } from 'redux';
import userInfo from './userInfo.reducer';
import pageTitle from './pageTitle.reducer';
import upcomingData from './reportsUpcomingData.reducer';
import upcomingDataLoader from './upcomingDataLoader.reducer';
import notifications from './notifications.reducer';
import gmex from './gmex.reducer';
import query from './query.reducer';

export const rootReducer = combineReducers({
  userInfo,
  pageTitle,
  upcomingData,
  upcomingDataLoader,
  notifications,
  gmex,
  query,
});
